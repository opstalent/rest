{
  "name" : "opstalent/rest",
  "description" : "REST Bundle",
  "type" : "symfony-bundle",
  "authors" : [
    {
      "name" : "Szymon Kunowski",
      "email" : "szymon.kunowski@opstalent.it",
      "homepage": "https://github.com/auamarto"
    },
    {
      "name" : "Patryk Grudniewski",
      "email" : "patgrudniewski@gmail.com",
      "homepage": "https://github.com/patgrudniewski"
    }
  ],
  "keywords" : [
    "symfony 3.4", "rest"
  ],
  "license" : [
    "MIT"
  ],
  "require" : {
    "opstalent/crud": "1.*"
  },
  "require-dev" : {
    "phpunit/phpunit": "^6.2",
    "matthiasnoback/symfony-dependency-injection-test": "^2.2",
    "squizlabs/php_codesniffer": "^3.1.1"
  },
  "suggest": {
  },
  "autoload" : {
    "psr-0" : {
      "Opstalent\\RestBundle" : "src/"
    },
    "psr-4": {
      "Opstalent\\RestBundle\\" : "src/"
    }
  },
  "autoload-dev" : {
    "psr-0" : {
      "Opstalent\\RestBundle\\Tests\\" : "tests/"
    },
    "psr-4": {
      "Opstalent\\RestBundle\\Tests\\" : "tests/"
    }
  },
  "repositories" : [{
    "type" : "vcs",
    "url" : "https://github.com/opstalent/rest.git",
    "no-api": true
  }]

}
